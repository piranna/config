{
  "name": "unify-config",
  "version": "0.3.1",
  "description": "Unify config from `package.json`, `.env` file, environment variables and argv",
  "main": "lib",
  "browser": "browser.js",
  "scripts": {
    "coveralls": "cat coverage/lcov.info | coveralls",
    "test": "jest --coverage --notify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piranna/unify-config.git"
  },
  "keywords": [
    "npm_package_config",
    "env",
    ".env",
    "argv",
    "config",
    "environment",
    "variables",
