    "dotenv",
    "ini",
    "json",
    "json5",
    "toml",
    "yaml"
  ],
  "author": "Jesús Leganés-Combarro 'piranna' <piranna@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/piranna/unify-config/issues"
  },
  "homepage": "https://github.com/piranna/unify-config#readme",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "caller-callsite": "piranna/caller-callsite",
    "deepmerge": "^2.1.1",
    "minimist": "^1.2.0",
    "object-recursive-iterator": "^1.0.2",
    "pkg-dir": "^3.0.0",
    "read-pkg-up": "^4.0.0",
    "string2js": "^0.2.0",
    "urlsearchparams-to-cli": "git+ssh://git@bitbucket.org/piranna/urlsearchparams-to-cli.git"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "dotenv": "^6.0.0",
    "jest-cli": "^23.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js",
      "lib/*.js"
    ]
